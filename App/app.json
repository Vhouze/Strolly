{
  "expo": {
    "name": "Trendby",
    "slug": "Trendby",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon2.png",
    "splash": {
      "image": "./assets/splash4.png",
      "resizeMode": "cover",
      "backgroundColor": "#EBBE14"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.trendby.Mvp",
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon2.png",
        "backgroundColor": "#EBBE14"
      },
      "package": "com.trendby.Mvp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
